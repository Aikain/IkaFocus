{
    "$schema": "http://json.schemastore.org/prettierrc",
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSameLine": false,
    "importOrder": [
        "^((react|next)(.*)$)|^((react|next)$)",
        "<THIRD_PARTY_MODULES>",
        "^(@mui/material/(.*)$)",
        "^(@mui/icons-material/(.*)$)",
        "^@/(components|plugins)/(.*)$",
        "^@/(lib|utils)/(.*)$",
        "^@/hooks/(.*)$",
        "^@/types/(.*)$",
        "^@/styles/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ]
}
